{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:optimize": "node scripts/build-optimize.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test:browser": "python3 test-browser.py",
    "export": "next build",
    "deploy": "next build",
    "deploy:cloudflare": "next build && npx wrangler pages deploy out --project-name=YOUR_PROJECT_NAME",
    "deploy:github": "next build && touch out/.nojekyll && echo 'p8552015.github.io' > out/CNAME",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out node_modules/.cache",
    "auto-commit": "node scripts/auto-commit.js"
  },
  "dependencies": {
    "next": "15.3.5",
    "playwright": "^1.53.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
}
